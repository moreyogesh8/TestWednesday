<aura:component >
    <aura:attribute name="contacts" type="Contact[]" />
    
    <li>Available Contacts</li>
    
    <ul>
    	<aura:iteration items="{!v.contacts}" var="con">
            <li type="dice">
                ContactName: {!con.Name} <hr/>
            </li>
        </aura:iteration>    
    </ul>
</aura:component>