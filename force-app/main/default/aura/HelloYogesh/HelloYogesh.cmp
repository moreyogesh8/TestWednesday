<aura:component >
    <h1>Hello Yogesh</h1>
    <div>
    	<div class="red">I am red</div>
        <div class="blue">I am blue</div>
        <div class="green">I am green</div>
    </div>
    <br/><br/>
    <aura:attribute name="num1" type="Integer" default="110" />
    <aura:attribute name="num2" type="Integer" default="20" />
    <aura:attribute name="sum" type="Integer" />
    {!v.num1} + {!v.num2} = {!v.sum}
    <br/>
    <ui:button label="Press Me/Click Me" press="{!c.add}"/>

    <aura:attribute name="message" type="String"/>
	<p>{!$Label.c.Greeting +' '+ v.message}</p>    
</aura:component>